{
  "name": "notion-blog",
  "version": "0.1.0",
  "scripts": {
    "dev": "next dev -p 4000",
    "start": "next start -p 4001",
    "build": "next build && node .next/serverless/scripts/build-rss.js",
    "format": "prettier --write \"**/*.{js,jsx,json,ts,tsx,md,mdx,css,html,yml,yaml,scss,sass}\" --ignore-path .gitignore",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "@tailwindcss/typography": "^0.3.1",
    "@zeit/react-jsx-parser": "^2.0.0",
    "async-sema": "^3.1.0",
    "autoprefixer": "^10.1.0",
    "axios": "^0.21.0",
    "focus-visible": "^5.2.0",
    "github-slugger": "^1.2.1",
    "katex": "^0.12.0",
    "next": "^10.0.2",
    "postcss": "^8.2.1",
    "prismjs": "^1.17.1",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "tailwindcss": "^2.0.2",
    "uuid": "^8.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/katex": "^0.11.0",
    "@types/node": "^13.1.2",
    "@types/react": "^16.9.17",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "eslint": "^7.14.0",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "postcss-focus-visible": "^5.0.0",
    "prettier": "^2.2.0",
    "typescript": "^4.1.2"
  }
}
